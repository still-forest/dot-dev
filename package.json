{
  "name": "still-forest-dot-dev",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently \"yarn workspace api dev\" \"yarn workspace web dev\"",
    "build": "yarn workspace api build && yarn workspace web build",
    "docker:build": "docker-compose build",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --write --unsafe .",
    "test": "concurrently \"yarn workspace api test\" \"yarn workspace web test\"",
    "type-check": "yarn workspaces foreach run type-check"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.4",
    "@still-forest/biomejs-config": "1.3.0",
    "concurrently": "8.2.2"
  },
  "packageManager": "yarn@4.7.0+sha512.5a0afa1d4c1d844b3447ee3319633797bcd6385d9a44be07993ae52ff4facabccafb4af5dcd1c2f9a94ac113e5e9ff56f6130431905884414229e284e37bb7c9"
}
