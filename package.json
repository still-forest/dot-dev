{
  "name": "still-forest-dot-dev",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm --filter ./apps/api dev\" \"pnpm --filter ./apps/web dev\"",
    "build": "pnpm --filter ./apps/api build && pnpm --filter ./apps/web build",
    "docker:build": "docker-compose build",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --write --unsafe .",
    "test": "concurrently \"pnpm --filter ./apps/api test\" \"pnpm --filter ./apps/web test\"",
    "type-check": "pnpm --filter '*' run type-check"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.4",
    "@still-forest/biomejs-config": "1.3.0",
    "concurrently": "8.2.2"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
