{
  "name": "still-forest-dot-dev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "pnpm run build:client && pnpm run build:server",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "dev": "concurrently \"pnpm dev:client\" \"pnpm dev:server\"",
    "dev:client": "vite",
    "dev:server": "NODE_ENV=development tsx watch server/index.ts",
    "dev:server:debug": "tsx watch --inspect server/index.ts",
    "dev:server:debug:vite": "concurrently \"vite\" \"tsx watch --inspect server/index.ts\"",
    "format": "biome format --write .",
    "lint": "biome check .",
    "lint:ci": "biome ci .",
    "lint:fix": "biome check --write --unsafe .",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit && tsc -p tsconfig.server.json --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@still-forest/canopy": "^0.21.0",
    "@tailwindcss/vite": "^4.1.6",
    "concurrently": "^9.1.2",
    "express": "^5.1.0",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "tailwindcss": "^4.1.7",
    "zod": "^3.25.20"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.4",
    "@still-forest/biomejs-config": "1.3.0",
    "@svgr/rollup": "8.1.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/express": "5.0.2",
    "@types/node": "22.15.18",
    "@types/react": "19.1.5",
    "@types/react-dom": "19.1.5",
    "@vitejs/plugin-react-swc": "3.9.0",
    "@vitest/coverage-v8": "3.1.4",
    "jsdom": "26.1.0",
    "msw": "2.8.4",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vitest": "3.1.4"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
